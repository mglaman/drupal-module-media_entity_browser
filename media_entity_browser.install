<?php

/**
 * @file
 * Install file for media entity browser  module.
 */

use Drupal\image\Entity\ImageStyle;

/**
 * Implements hook_uninstall().
 */
function media_entity_browser_uninstall() {
  $imageStyle = ImageStyle::load('media_entity_browser_thumbnail');
  if (!empty($imageStyle)) {
    $imageStyle->delete();
  }
}
